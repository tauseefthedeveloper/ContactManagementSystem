package com.mappingXML;

public class Person {
	private int id;
	private String name;
	private String city;
	private String phone;
	
	public Person() {
		super();
	}
	public Person(int id, String name, String address, String phone) {
		super();
		this.id = id;
		this.name = name;
		this.city = address;
		this.phone = phone;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAdress() {
		return city;
	}
	public void setAdress(String address) {
		this.city = address;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	
}
